{
  "name": "careless-whisper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run generate-md && next dev --turbopack -p 3001",
    "build": "next build",
    "export-gh-pages": "npm run generate-md && rm -rf out && cross-env TARGET=gh-pages next build && cp CNAME out",
    "generate-md": "node scripts/generate-md-json.js",
    "only-pub": "gh-pages -d out --nojekyll",
    "pub": "npm run export-gh-pages && npm run only-pub",
    "export": "rm -rf out && cross-env EXPORT=true next build",
    "serve": "serve out -p 4001",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "15.3.3",
    "antd": "^5.25.4",
    "antd-style": "^3.7.1",
    "classnames": "^2.3.2",
    "gray-matter": "^4.0.3",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "next": "15.3.3",
    "next-with-less": "^3.0.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "webpack": "^5.89.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@types/mdx": "^2.0.10",
    "@types/node": "^20",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.6",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "15.3.3",
    "gh-pages": "^6.1.0",
    "glob": "^11.0.2",
    "serve": "^14.2.1",
    "typescript": "^5"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.1.6",
      "@types/react-dom": "19.1.6"
    }
  }
}
