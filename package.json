{
  "name": "careless-whisper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "export-gh-pages": "rm -rf out && cross-env TARGET=gh-pages next build",
    "only-pub": "gh-pages -d out --nojekyll",
    "pub": "npm run export-gh-pages && gh-pages -d out --nojekyll",
    "export": "rm -rf out && cross-env EXPORT=true next build",
    "serve": "serve out -p 4001",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@next/mdx": "^14.0.4",
    "classnames": "^2.3.2",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "next": "14.0.4",
    "next-with-less": "^3.0.1",
    "react": "^18",
    "react-dom": "^18",
    "webpack": "^5.89.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@types/mdx": "^2.0.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "gh-pages": "^6.1.0",
    "serve": "^14.2.1",
    "typescript": "^5"
  }
}
